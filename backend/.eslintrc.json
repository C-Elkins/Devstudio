{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": ["warn", { "args": "after-used", "argsIgnorePattern": "^_" }],
    "no-undef": "error",
    "no-console": "off",
    "prefer-const": "warn",
    "eqeqeq": ["warn", "smart"]
  }
}
